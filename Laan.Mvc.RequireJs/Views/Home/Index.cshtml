@{
    ViewBag.Title = "Home Page";
}

<h2>Fibonacci Series</h2>

<table>
    <thead>
        <tr>
            <th>Number</th>
            <th>Fibonacci</th>
        </tr>
    </thead>
    <tbody data-bind="foreach: Series">
        <tr>
            <td data-bind="text: $index"></td>
            <td data-bind="text: $data"></td>
        </tr>
    </tbody>
</table>
